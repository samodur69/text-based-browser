(function(el){
	if(el.length>1) {
		$(el.get(1)).find('a[href][data-utm]').each(function(){
			var a=$(this);
			a.attr('data-utm',a.attr('data-utm').replace('utm_medium=news-right-block','utm_medium=floating-news-right-block'));
		});
	}
	runCore.init_tracking(el);
